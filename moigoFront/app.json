{
  "expo": {
    "name": "spotple",
    "slug": "spotple",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/spotple.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/spotple.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sehankim.spotple",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIViewControllerBasedStatusBarAppearance": false,
        "UIStatusBarStyle": "light-content",
        "UIRequiresFullScreen": false,
        "UISupportedInterfaceOrientations": ["UIInterfaceOrientationPortrait"],
        "UISupportedInterfaceOrientations~ipad": ["UIInterfaceOrientationPortrait", "UIInterfaceOrientationPortraitUpsideDown", "UIInterfaceOrientationLandscapeLeft", "UIInterfaceOrientationLandscapeRight"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/spotple.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sehankim.spotple"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3ddd0517-7ac9-447f-85f3-60927f4aff61"
      },
      "API_URL": "https://spotple.kr",
      "WS_URL": "wss://spotple.kr"
    }
  }
}
