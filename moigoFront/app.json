{
  "expo": {
    "name": "spotple",
    "slug": "moigoFront",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/spotple-icon.png",
    "userInterfaceStyle": "light",
    "extra": {
      "eas": {
        "projectId": "3ddd0517-7ac9-447f-85f3-60927f4aff61"
      }
    },
    "scheme": "moigo",
    "splash": {
      "image": "./assets/spotple-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "icon": "./assets/spotple-icon.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.sehankim.spotple",
      "associatedDomains": [
        "applinks:moigo.com"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "갤러리 접근을 허용해야 사진 업로드가 가능합니다.",
        "NSCameraUsageDescription": "카메라 접근을 허용해야 사진 촬영이 가능합니다."
      }
    },
    "android": {
      "icon": "./assets/spotple-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/spotple-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sehankim.moigoFront",
      "versionCode": 1,
      "supportsTablet": true,
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.WAKE_LOCK",
        "android.permission.ACCESS_NETWORK_STATE",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.READ_MEDIA_VIDEO",
        "android.permission.CAMERA"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "moigo",
              "host": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3ddd0517-7ac9-447f-85f3-60927f4aff61"
      },
      "API_URL": "https://spotple.kr",
      "WS_URL": "wss://spotple.kr"
    },
    "owner": "sehankim",
    "platforms": ["ios", "android", "web"]
  }
}
